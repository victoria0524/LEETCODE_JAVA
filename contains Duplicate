class Solution {
    public boolean containsDuplicate(int[] nums) 
    {
        HashMap<Integer, Integer> map=new HashMap<Integer, Integer>();
        if(nums.length<1)
            return false;
        
        map.put(nums[0],1);
        
        for(int i=1;i<nums.length;i++)
        {   int temp=nums[i];
            if(map.get(temp) != null)
                return true;
            else
                map.put(temp,1);
        }
        return false;
    }
}
